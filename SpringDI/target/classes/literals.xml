<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->
<bean id="e1" class="org.tnsif.literals.Employee">
	<constructor-arg name="empid" value="104"></constructor-arg>
	<constructor-arg name="empname" value="Abhinav Rawat"></constructor-arg>
	<constructor-arg name="address" value="Delhi"></constructor-arg>
	<constructor-arg name="salary" value="100000"></constructor-arg>

</bean>
<bean id="e2" class="org.tnsif.literals.Employee">
	<property name="empid" value="101"></property>
	<property name="empname" value="Abhinav Rawat"></property>
	<property name="address" value="Mumbai"></property>
	<property name="salary" value="3200"></property>
</bean>
</beans>